                                    <TextBox x:Name="textBox_RetM" HorizontalAlignment="Stretch"  Margin="155,30,40,20" Grid.Row="0" TextWrapping="Wrap" PlaceholderText="Hvad skal der spises" VerticalAlignment="Stretch" IsReadOnly="True" Text="{Binding uge.mandag.Ret, Mode=TwoWay}"/>
                                    <TextBox x:Name="textBox_ChefKokM" HorizontalAlignment="Stretch"  Margin="155,30,40,20" Grid.Row="0" TextWrapping="Wrap" PlaceholderText="Hvem har ansvar for Køkkenet" VerticalAlignment="Stretch" Grid.Column="1" IsReadOnly="True" Text="{Binding uge.mandag.ChefKok, Mode=TwoWay}"/>
