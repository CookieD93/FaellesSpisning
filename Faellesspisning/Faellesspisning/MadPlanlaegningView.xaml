                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="9*"/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="{Binding uge.StrUgenummer}" FontSize="30" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="50,0,34,10" Grid.Row="0" Height="auto" Width="auto"/>
