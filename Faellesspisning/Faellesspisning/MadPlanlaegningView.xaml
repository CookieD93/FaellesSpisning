                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1.25*"/>
                                        <RowDefinition Height="2*"/>
                                        <RowDefinition Height="3*"/>


                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock x:Name="textBlock_RetM" FontSize="24" HorizontalAlignment="Left" Height="auto" Margin="50,0,0,0" TextWrapping="Wrap" Text="Ret:" VerticalAlignment="Center" Width="Auto" Grid.Row="0"/>
